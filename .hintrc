{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-app-region",
          "user-select"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ]
  }
}